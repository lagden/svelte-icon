{
  "name": "@tadashi/svelte-icon",
  "version": "1.1.0",
  "repository": "github:lagden/svelte-icon",
  "description": "Svelte component",
  "keywords": [
    "svelte",
    "icon",
    "component"
  ],
  "svelte": "src/Icon.svelte",
  "main": "src/icon.js",
  "type": "module",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=14.16.0"
  },
  "scripts": {
    "eslint": "eslint --config .eslintrc.cjs --ext .js --ignore-path .gitignore .",
    "rm": "rm -rf dist",
    "esbuild": "node ./build.js",
    "build": "run-s test rm esbuild",
    "prepublishOnly": "npm run build",
    "pretest": "npm run eslint",
    "test": "jest test --coverage --coverageReporters=lcov --coverageReporters=text",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/eslint-parser": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@testing-library/svelte": "3.0.3",
    "babel-jest": "27.4.5",
    "esbuild": "0.14.7",
    "esbuild-svelte": "0.6.0",
    "eslint": "8.5.0",
    "eslint-config-xo": "0.39.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-svelte3": "3.2.1",
    "eslint-plugin-unicorn": "39.0.0",
    "jest": "27.4.5",
    "npm-run-all": "4.1.5",
    "svelte": "3.44.3",
    "svelte-jester": "2.1.5"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
